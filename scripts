loadstring(game:HttpGet("https://raw.githubusercontent.com/jinkelatom/roblox-scripts1/main/remote%20spy"))()
-- spy -- https://raw.githubusercontent.com/jinkelatom/roblox-scripts1/main/remote%20spy 
-- Dex Explorer -- https://raw.githubusercontent.com/jinkelatom/roblox-scripts1/main/Dex%20Explorer
-- Infinite Yield -- https://raw.githubusercontent.com/jinkelatom/roblox-scripts1/main/Infinite%20Yield


\/ \/ rapid fire \/ \/

local RemotesName = "RemoteEvent" 
local Number = 1 -- change how much want to repeat

local settings = {repeatamount = Number, inclusions = {RemotesName}}
local mt = getrawmetatable(game)
local old = mt.__namecall
setreadonly(mt, false)

mt.__namecall = function(uh, ...)
 local args = {...}
 local method = getnamecallmethod()
 for i,o in next, settings.inclusions do
     if uh.Name ~= o then
         return old(uh, ...)
     end
 end
 if method == "FireServer" or method == "InvokeServer" then
     for i = 1,settings.repeatamount do
         old(uh, ...)
     end
 end
 return old(uh, ...)
end

setreadonly(mt, true)
